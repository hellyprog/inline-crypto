{
    "name": "Inline Crypto",
    "description": "Extension for getting inline crypto info on page.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "scripts/background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html"
    }
}